<!--해당문서가 html5 버전으로 작성되었음을 정의-->
<!DOCTYPE html>

<!--html 태그: 해당문서가 html문서임을 정의-->
<html>
	<!--head 태그: 해당 문서의 정보-->
	<head>
	</head>
	
	<!--body 태그: 실제 브라우저에 표시되는 내용-->
	<body>
		<!-- li 태그: 리스트를 만들어줌 -->
		<!-- ol태그 안에 위치시 순서가 있는 리스트 생성 -->
		<!-- ul태그 안에 위치시 순서가 없는 리스트 생성 -->
		<ol>
			<li>첫째</li>
			<li>둘째</li>
			<li>셋째</li>	
		</ol>
		
		<ul>
			<li>첫째</li>
			<li>둘째</li>
			<li>셋째</li>	
		</ul>
	</body>
</html>
